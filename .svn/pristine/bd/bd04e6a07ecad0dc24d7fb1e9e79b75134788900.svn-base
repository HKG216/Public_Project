            $(".ptcard_text").html("");
        }

        function fn_ClearNoNum(e, pnumber) {
            if (!/^\d+?\d*$/.test(pnumber)) {
                $(e).val(/^\d+?\d*/.exec($(e).val()));
            }
            return false;
        }



